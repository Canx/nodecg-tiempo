{
  "name": "nodecg-tiempo",
  "version": "0.1",
  "description": "contador de tiempo para partido de futbol",
  "author": "Ruben Cancho <canchete@gmail.com>",
  "nodecg": {
    "compatibleRange": "~0.9.0",
    "graphics": [
      {
        "file": "tiempo.html",
        "width": 600,
        "height": 100,
        "singleInstance": false
      },
      {
        "file": "marcador.html",
        "width": 1200,
        "height": 300
      }
    ],
    "dashboardPanels": [
      {
        "name": "smallscore",
        "title": "Marcador peque√±o",
        "width": 2,
        "file": "tiempo.html",
        "headerColor": "#9f9bbd"
      }
    ],

    "soundCues": [
      {
        "name": "gol",
        "channels": 1,
        "defaultFile": "sound/gol.mp3",
        "defaultVolume": 80
      }
    ]
